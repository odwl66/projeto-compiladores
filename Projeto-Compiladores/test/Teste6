program exFunction;

var
   a, b, c,d, num1, num2, max, ret: integer;

function max(var num1, num2, num3: integer; var b: String): integer;

var
   result: integer;
   
begin
	num1 := 1;
	num2 := 2;
	max := 2;
   if (num1 > num2) then
      result := num1
   else
      result := num2;
   max := result;
end;

begin
   a := 100;
   b := 200;
   c := 300;
   d := 'casa';
