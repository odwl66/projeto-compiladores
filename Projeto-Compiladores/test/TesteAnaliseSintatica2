program p1;
var W, X, Y, Z, number, sum: Integer;

begin 
1:	X := 10;
2:	Y := 2;
3: goto 2;
4: Z := 3;

if (X > 0) then
	Z := 3;
	
while X > 0 do
begin
   X := X - 2;
end;
	
case X of
	1: Y := 3;
end;

for W := 10  to 20 do
   begin
      X := X + 2;
   end;
   
with X, Y do X := Y;

repeat
   Y := Y - 2;
until number = 0;


end
.