program p1(ola, hello);
label 1, +2, -3;
const a1 = 1.1;
	b1 = 'ola';
	b2 = '(xau)';
	c1 = -1.2;
	c2 = +2.4;
	d1 = -a4;
	d2 = +a5;
	d3 = d2;
	e1 = 23;
	e2 = -2;
	e3 = +45;
type t1 = a1 .. b1;
	t2 = 1 .. 'ola';
	t3 = 1.1 .. 1;
	t4 = (a1, b1, b2); 
	t5 = packed array [1 .. 5, (r1, r3)] of (r2);
	t5 = array [1 .. 5, (r1, r3)] of (r2);
	t6 = packed record a1, a2, a8 : 1 .. 2 end;
	t7 = record a1, a2, a8 : 1 .. 2 end;
	t8 = packed record a1, a2, a8 : (a2, a3) end;
	t9 = record a1, a2, a8 : (a2, a3) end;
	t10 = set of 1 .. 2;
	t11 = set of (a2, a3);
	t12 = file of 1 .. 2;
	t13 = file of (a2, a3);
	t14 = ^a1;
.